 <body class="d-flex flex-column min-vh-100">
 <body>
    <body class="d-flex flex-column min-vh-100">
      <header class="stick-top">
        <nav class="navbar navbar-expand-lg navbar-dark">
        <!-- Container wrapper -->
          <div class="container">
            
            <!-- Toggle button -->
            <a class="navbar-brand">
              <img
              src="./assets/Logo 650x650 px.jpeg"
              alt=""
              width="150"
              height="100"
              class="d-inline-block align-text-left text-white"
            />
          </a>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <ul class="navbar-nav ml-auto">
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <!-- Left links -->
          <ul class="navbar-nav ms-auto mb-2 mb-lg-0">
            <li class="nav-item">
              <a class="nav-link d-flex flex-column text-center" aria-current="page" href="/"><i class="fas fa-home fa-lg"></i><span class="mt-4">Home</span></a>
            </li>
            <li class="nav-item">
              <a class="nav-link d-flex flex-column text-center" aria-current="page" href="/dashboard"><i class="far fa-comment-dots fa-lg"></i><span class="mt-4">Dashboard</span></a>
            </li>
            <li class="nav-item">
              <a class="nav-link d-flex flex-column text-center" aria-current="page" href="/about"><i class="far fa-comment-dots fa-lg"></i><span class="mt-4">About</span></a>
            </li>
            <!-- If LoggedIn, Show these Links-->
            {{#if loggedIn}}
            <li class="nav-item">
              <a class="nav-link d-flex flex-column text-center" aria-current="page" href="#" onclick="logout()"><i class="fas fa-sign-in-alt fa-lg"></i><span class="mt-4">Log Out</span></a>
            </li>
            <!-- Else, Show these links -->
            {{else}}
            <li class="nav-item">
              <a class="nav-link d-flex flex-column text-center" aria-current="page" href="/login"><i class="fas fa-sign-in-alt fa-lg"></i><span class="mt-4">Log In</span></a>
            </li>
            <li class="nav-item">
              <a class="nav-link d-flex flex-column text-center" aria-current="page" href="/signup"><i class="fas fa-user-plus fa-lg"></i><span class="mt-4">Sign Up</span></a>
            </li>
            {{/if}}
          </ul>
          <!-- Left links -->
        </div>
        <!-- Collapsible wrapper -->
      </div>
    </nav>
          </ul>
        </div>
      </div>
    </nav>
